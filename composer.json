{
    "name": "tweedegolf/media-bundle",
    "description": "Media manager bundle for Symfony and tinyMCE",
    "license": "MIT",
    "authors": [
        {
            "name": "Marlon Baeten",
            "email": "marlon@tweedegolf.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "doctrine/doctrine-bundle": "^1.2",
        "doctrine/orm": "^2.4",
        "liip/imagine-bundle": "^1.2",
        "stof/doctrine-extensions-bundle": "^1.1",
        "symfony/symfony": "^2.7|^3.0",
        "vich/uploader-bundle": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.2",
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "TweedeGolf\\MediaBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
